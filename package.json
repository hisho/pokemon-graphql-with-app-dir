{
  "name": "rsc-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev -p 3000",
    "dev:codegen": "pnpm build:codegen --watch",
    "build": "run-s build:codegen build:next",
    "build:next": "next build",
    "build:codegen": "graphql-codegen",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:next": "next lint --dir src --ignore-path .prettierignore --max-warnings 0",
    "lint:typecheck": "tsc --pretty --noEmit",
    "lint:prettier": "prettier --check .",
    "format": "run-s format:*",
    "format:eslint": "pnpm lint:next --fix",
    "format:prettier": "pnpm lint:prettier --write"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@t3-oss/env-nextjs": "^0.4.1",
    "dotenv": "^16.3.1",
    "graphql": "^16.6.0",
    "graphql-request": "^6.1.0",
    "next": "13.4.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "ts-pattern": "^5.0.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "autoprefixer": "10.4.14",
    "encoding": "^0.1.13",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.6",
    "eslint-plugin-perfectionist": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.24",
    "prettier": "^2.8.8",
    "typescript": "5.1.3"
  }
}